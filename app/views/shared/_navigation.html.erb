<% if !current_user %>
  <%= link_to "Home", root_path %>
  <%= link_to "Sign In", sign_out_path %>
<% else %>
  <% if current_user.rank == 1 %>
    <%= link_to "My Challenges", choose_challenges_path %>
  <% elsif current_user.rank > 1 %>
    <%= link_to "User Challenges", challenges_path %>
    <%= link_to "User Gameplans", gameplans_path %>
    <%= link_to "Users", users_path %>
  <% elsif current_user.rank > 2 %>
    <%= link_to "Organisations", organisations_path %>
    <%= link_to "New Password", new_password_path %>
  <% end %>
  <div class="right_nav">
    <%= link_to "Sign Out", sign_out_path %>
  </div>
<% end %>